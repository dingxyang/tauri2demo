{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri2demo",
  "version": "0.1.0",
  "identifier": "com.tauri2demo.app",
  "build": {
    "frontendDist": "../../frontend/dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "pnpm start",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "tauri2demo",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "android": {
      "minSdkVersion": 26
    },
    "iOS": {
      "developmentTeam": "12345",
      "minimumSystemVersion": "14.0"
    }
  }
}
